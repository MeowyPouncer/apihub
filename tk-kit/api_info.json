{
  "base_url": "https://capi.tk-kit.com",
  "authorization": [
    "Bearer token in Authorization header",
    "token query parameter"
  ],
  "endpoints": [
    {
      "path": "/1.0/order/calculate-post",
      "method": "POST",
      "summary": "Расчет стоимости доставки посылки.",
      "request_fields": [
        {"name": "city_pickup_code", "description": "Код города откуда", "type": "string", "required": "Да", "example": "660000100000"},
        {"name": "city_delivery_code", "description": "Код города куда", "type": "string", "required": "Да", "example": "000000000001"},
        {"name": "declared_price", "description": "Объявленная стоимость груза (руб)", "type": "string", "required": "Да", "example": "1000"},
        {"name": "confirmation_price", "description": "Наличие документов подтверждающих стоимость", "type": "string", "required": "Нет", "example": "1"},
        {"name": "service", "description": "Массив кодов услуг", "type": "array[string]", "required": "Нет", "example": "[\"S089\"]"},
        {"name": "post_type", "description": "Тип посылки", "type": "string", "required": "Нет", "example": "02"}
      ],
      "response_fields": [],
      "example_request": "{ \"city_pickup_code\": \"660000100000\", \"city_delivery_code\": \"000000000001\" }",
      "example_response": "[ { \"S\": { \"cost\": 4284 } } ]"
    },
    {"path": "/1.0/order/cargo-type/get-list", "method": "GET", "summary": "Список типов груза"},
    {"path": "/1.0/order/currency/get-list", "method": "GET", "summary": "Список валют"},
    {"path": "/1.0/order/document/get", "method": "GET", "summary": "Получить документ по заказу"},
    {"path": "/1.0/order/get", "method": "GET", "summary": "Получить информацию о заказе"},
    {"path": "/1.0/order/get-list", "method": "GET", "summary": "Список заказов"},
    {"path": "/1.0/order/mass-create", "method": "POST", "summary": "Массовое создание заказов"},
    {"path": "/1.0/order/photo/get", "method": "GET", "summary": "Получить фотографии заказа"},
    {"path": "/1.0/order/service/get-list", "method": "GET", "summary": "Список услуг"},
    {"path": "/1.0/order/status/get", "method": "GET", "summary": "Статус заказа"},
    {"path": "/1.0/order/time-delivery", "method": "GET", "summary": "Сроки доставки"},
    {"path": "/1.1/order/create-post", "method": "POST", "summary": "Создание посылки (v1.1)"},
