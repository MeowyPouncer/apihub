    {"path": "/2.0/order/calculate", "method": "POST", "summary": "Расчет стоимости (v2.0)"},
    {"path": "/1.0/tdd/city/get-list", "method": "GET", "summary": "Список городов"},
    {"path": "/1.0/tdd/country/get-list", "method": "GET", "summary": "Список стран"},
