    { "path": "/1.5/courier", "method": "GET", "summary": "Данные и местоположение курьера" },
    { "path": "/1.5/client", "method": "GET", "summary": "Данные клиентского профиля" },
    { "path": "/1.5/bank-cards", "method": "GET", "summary": "Привязанные банковские карты" }
  ]
