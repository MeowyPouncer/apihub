{
  "base_url": "https://robotapi.dostavista.ru/api/business/1.5",
  "authorization": [
    "X-DV-Auth-Token header"
  ],
  "endpoints": [
    { "path": "/1.5/calculate-order", "method": "POST", "summary": "Расчет стоимости доставки" },
    { "path": "/1.5/create-order", "method": "POST", "summary": "Создание заказа" },
    { "path": "/1.5/orders", "method": "GET", "summary": "Получение списка заказов" },
    { "path": "/1.5/edit-order", "method": "POST", "summary": "Редактирование заказа" },
    { "path": "/1.5/cancel-order", "method": "POST", "summary": "Отмена заказа" },
    { "path": "/1.5/receipts", "method": "GET", "summary": "Детализация стоимости заказа" },
    { "path": "/1.5/delivery-intervals", "method": "GET", "summary": "Список интервалов доставки" },
